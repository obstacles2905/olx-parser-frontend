{
  "name": "olx-parser-frontend",
  "version": "2.3.3",
  "scripts": {
    "react:start": "react-scripts start",
    "start": "npm run react:start",
    "build": "react-scripts build",
    "test": "cross-env CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,jsx}' --quiet --fix",
    "linttest": "npm run lint & npm run test",
    "mock:server": "nodemon ./__mocks__/mock-server.js",
    "start:server": "nodemon server/server.js",
    "start:dev": "concurrently -k \"npm run start:devserver\" \" npm run start\"",
    "start:staging": "concurrently -k  \"npm run start:stagingserver\" \" PORT=3002 npm run react:start\"",
    "start:prod": "npm run build && npm run start:prodserver"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-flexbox-grid": "^2.1.2",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-promise-middleware": "^6.1.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "forever": "^1.0.0",
    "helmet": "^3.20.0",
    "http-proxy": "^1.17.0",
    "husky": "^3.0.2",
    "json-server": "^0.14.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "redux-devtools-extension": "^2.13.5",
    "yargs": "^13.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run linttest && git add .",
      "pre-push": "npm run linttest"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
